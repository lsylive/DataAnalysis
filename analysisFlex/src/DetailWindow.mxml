<?xml version="1.0" encoding="UTF-8"?>
<mx:TitleWindow xmlns:mx="http://www.adobe.com/2006/mxml" layout="vertical" close="{close();}" creationComplete="{init();}">
	<mx:Script><![CDATA[
        //警告: 在未注册版本中，导出Flex项目时不生成Flex脚本,要生成Flex脚本，请到http://www.shankejingling.com/goumai.html购买完整版。

    ]]></mx:Script>

	<mx:Panel id="queryTip" width="250" height="50" x="200" y="100" layout="vertical" includeInLayout="false" visible="false" borderStyle="solid" borderColor="5592405" paddingBottom="0" paddingLeft="0" paddingRight="0" paddingTop="0" cornerRadius="5" borderThickness="1" modalTransparencyBlur="1" modalTransparency="0.15" modalTransparencyColor="12312063" modalTransparencyDuration="400" roundedBottomCorners="true" headerHeight="0" verticalAlign="middle">
		<mx:HBox height="100%" width="100%" verticalAlign="middle" horizontalGap="2" borderStyle="none" paddingLeft="5">
			<mx:Image width="32" height="32" source="icons/loading.swf"/>
			<mx:Label text="正在查询数据，请稍候......" fontSize="14" fontWeight="bold"/>
		</mx:HBox>
	</mx:Panel>
	<mx:RecordWindow id="recordBox" visible="false" includeInLayout="false" width="60%" height="80%" goClose="{recordBox.visible = false;}" hideEffect="zoom"/>
	<mx:VBox id="main" width="100%" height="100%" verticalScrollPolicy="off" horizontalScrollPolicy="off" horizontalAlign="center" borderStyle="none" cornerRadius="0" paddingBottom="1" paddingLeft="0" paddingRight="0" paddingTop="0" verticalGap="1" backgroundAlpha="1" backgroundColor="#00e3f1ef">
		<mx:DataGrid id="dg" styleName="detailGrid" width="100%" height="100%" doubleClickEnabled="true" columns="{[_DetailWindow_DataGridColumn1_c(), _DetailWindow_DataGridColumn2_c(), _DetailWindow_DataGridColumn3_c()]}" itemDoubleClick="{dg_itemDoubleClickHandler(event);}"/>
		<mx:HBox id="recCount" height="35" width="100%" verticalAlign="middle" horizontalAlign="left">
			<mx:Label id="WinCount" width="50%" fontSize="12" textAlign="left" fontWeight="normal"/>
			<mx:Button label="关闭" width="80" height="25" icon="_embed_mxml_icons_Exit_png_932773307" click="{goClose(event);}"/>
			<mx:Label width="50%" fontSize="12" textAlign="left" fontWeight="normal"/>
		</mx:HBox>
	</mx:VBox>

	<mx:Style>/* //警告: 在未注册版本中，导出Flex项目时不生成Flex脚本,要生成Flex脚本，请到http://www.shankejingling.com/goumai.html购买完整版。
 */</mx:Style>
</mx:TitleWindow>
