<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.liusy.datapp.model.datastandard">
   <class name="StandardCodeset" table="T_STANDARD_CODESET" >
      <meta attribute="sync-DAO">true</meta>
      <id name="id" column="ID" type="integer" >
         <generator class="native"/>
      </id>
      <property name="modifyDate" column="MODIFY_DATE" type="date" not-null="false" length="7" />
      <property name="type" column="TYPE" type="string" not-null="false" length="2" />
      <property name="categoryId" column="CATEGORY_ID" type="integer" not-null="false" length="22" />
      <property name="codeExpression" column="CODE_EXPRESSION" type="string" not-null="false" length="256" />
      <property name="createBy" column="CREATE_BY" type="string" not-null="false" length="32" />
      <property name="modifyBy" column="MODIFY_BY" type="string" not-null="false" length="32" />
      <property name="createDate" column="CREATE_DATE" type="date" not-null="false" length="7" />
      <property name="standardCode" column="STANDARD_CODE" type="string" not-null="false" length="64" />
      <property name="status" column="STATUS" type="string" not-null="false" length="1" />
      <property name="encodeRule" column="ENCODE_RULE" type="string" not-null="false" length="128" />
      <property name="codesetNo" column="CODESET_NO" type="string" not-null="false" length="64" />
      <property name="name" column="NAME" type="string" not-null="false" length="64" />
      <property name="remark" column="REMARK" type="string" not-null="false" length="256" />
   </class>	
</hibernate-mapping>
