<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.liusy.datapp.model.query">
   <class name="SubjectColumnCfg" table="T_QUERY_SUBJECT_COLUMN_CFG" >
      <meta attribute="sync-DAO">true</meta>
      <id name="id" column="ID" type="integer" >
         <generator class="native"/>
      </id>
      <property name="stQuery" column="ST_QUERY" type="string" not-null="false" length="1" />
      <property name="filterOperator" column="FILTER_OPERATOR" type="string" not-null="false" length="32" />
      <property name="batchQuery" column="BATCH_QUERY" type="string" not-null="false" length="1" />
      <property name="indId" column="IND_ID" type="integer" not-null="true" length="22" />
      <property name="codeSetNo" column="CODESET_NO" type="string" not-null="false" length="64" />
      <property name="name" column="NAME" type="string" not-null="false" length="32" />
      <property name="cnName" column="CN_NAME" type="string" not-null="false" length="32" />
      <property name="dataType" column="DATA_TYPE" type="string" not-null="false" length="32" />
      <property name="homonymQuery" column="HOMONYM_QUERY" type="string" not-null="false" length="1" />
      <property name="filterValue" column="FILTER_VALUE" type="string" not-null="false" length="1024" />
      <property name="isFilter" column="IS_FILTER" type="string" not-null="false" length="1" />
      <property name="subjectId" column="SUBJECT_ID" type="integer" not-null="false" length="22" />
      <property name="orderNo" column="ORDER_NO" type="integer" not-null="false" length="22" />
      <property name="fuzzyQuery" column="FUZZY_QUERY" type="string" not-null="false" length="1" />
   </class>	
</hibernate-mapping>
