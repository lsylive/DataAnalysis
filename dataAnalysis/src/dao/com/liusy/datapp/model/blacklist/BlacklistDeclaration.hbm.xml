<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.liusy.datapp.model.blacklist">
   <class name="BlacklistDeclaration" table="T_BLACKLIST_DECLARATION" >
      <meta attribute="sync-DAO">true</meta>
      <id name="id" column="ID" type="integer" >
         <generator class="native"/>
      </id>
      <property name="appId" column="APP_ID" type="integer" not-null="false" length="22" />
      <property name="indCode" column="IND_CODE" type="string" not-null="false" length="32" />
      <property name="queryValue" column="QUERY_VALUE" type="string" not-null="false" length="4000" />
      <property name="decTime" column="DEC_TIME" type="date" not-null="false" length="7" />
      <property name="userId" column="USER_ID" type="integer" not-null="false" length="22" />
      <property name="startDate" column="START_DATE" type="date" not-null="false" length="7" />
      <property name="decCode" column="DEC_CODE" type="string" not-null="false" length="32" />
      <property name="remark" column="REMARK" type="string" not-null="false" length="256" />
      <property name="endDate" column="END_DATE" type="date" not-null="false" length="7" />
   </class>	
</hibernate-mapping>
