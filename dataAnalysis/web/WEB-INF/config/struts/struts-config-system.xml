      <forward name="ADD" path="/pages/system/org/sysUser_add.jsp" redirect="false" />
      <forward name="EDIT" path="/pages/system/org/sysUser_edit.jsp" redirect="false" />
      <forward name="VIEW" path="/pages/system/org/sysUser_view.jsp" redirect="false" />
