    <action path="/user/login" name="loginForm" scope="request" validate="true"
            type="com.liusy.datapp.web.login.LoginAction" unknown="false" >
      <forward name="LIST" path="/login.jsp" redirect="false" />
      <forward name="INDEX" path="/main_new.jsp" redirect="false" />
    </action>
     <action path="/user/logout" name="loginForm" scope="request" validate="true"
            type="com.liusy.datapp.web.login.LogOutAction" unknown="false" >
      <forward name="logout" path="/login.jsp" redirect="true" />
    </action>
   
